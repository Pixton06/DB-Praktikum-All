CREATE TABLE Restaurant (
	Name VARCHAR (20) PRIMARY KEY NOT NULL,
	Bestellungsgeschichte TIMESTAMP,
	PLZ INT NOT NULL,
	Passwort VARCHAR (20) NOT NULL,
	Öffnungszeiten TIMESTAMP NOT NULL,
	Beschreibung VARCHAR (100) NOT NULL,
	Foto
);

