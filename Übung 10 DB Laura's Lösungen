--Aufgabe 2a) ohne Assistenten BIN MIR NICHT SICHER OB ES RICHTIG IST 

select tmp.Name, tmp.AnzahlEintr채ge,
from (select s.name, s.semester, count(*) as AnzahlEintr채ge
			from Studenten s, hoeren h, pruefen p, 
			where s.matrnr = h.matrnr and s.matrnr = p.matrnr
			order by asc tmp.AnzahlEintr채ge) tmp
union
from (select pr.name,
			from Professoren pr,vorlesungen v, assistenten a, pruefen p, count(*) as AnzahlEintr채ge
			where pr.persnr = v.gelesenvon and pr.persnr = a.boss
				and pr.persnr = p.persnr

